{
  "manifest_version": 2,

  "name": "Ex-dividend Date Lookup",
  "description": "This extension uses the Yahoo! Finance API to provide information related to ex-dividend and dividend dates for stocks.",
  "version": "0.1",
   "content_scripts": [
    {
      "matches": ["<all_urls>"],
      //"css": ["mystyles.css"],
      "js": ["jquery.js","contentScript.js"]
    }
  ],
  "options_ui": {
    // Required.
    "page": "options.html",
    // Recommended.
    "chrome_style": true
  },
  "icons": { "16": "icon.png",
           "48": "icon.png",
          "128": "icon.png" },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popupDiv.html",
	"default_title": "Lookup Ex-dividend Date"
  },
  "background": {
  "scripts": ["background.js"]
},
  
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
	"storage"
  ]
}
